{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/Paginas_HTML/index.html"
    },
    {
      "src": "/Paginas_HTML/(.*)",
      "dest": "/Paginas_HTML/$1"
    },
    {
      "src": "/Paginas_CSS/(.*)",
      "dest": "/Paginas_CSS/$1"
    },
    {
      "src": "/Paginas_JS/(.*)",
      "dest": "/Paginas_JS/$1"
    },
    {
      "src": "/Imagens/(.*)",
      "dest": "/Imagens/$1"
    },
    {
      "src": "/Icones/(.*)",
      "dest": "/Icones/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 