{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/Paginas_HTML/Pagina_princial.html"
    },
    {
      "source": "/favicon.ico",
      "destination": "/Icones/favicon.ico"
    },
    {
      "source": "/Paginas_HTML/(.*)",
      "destination": "/Paginas_HTML/$1"
    },
    {
      "source": "/Paginas_CSS/(.*)",
      "destination": "/Paginas_CSS/$1"
    },
    {
      "source": "/Paginas_JS/(.*)",
      "destination": "/Paginas_JS/$1"
    },
    {
      "source": "/Imagens/(.*)",
      "destination": "/Imagens/$1"
    },
    {
      "source": "/Icones/(.*)",
      "destination": "/Icones/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 